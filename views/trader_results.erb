<% @results.each do |result| %>
	<% trader = result[0] %>
	<% stats = result[1] %>

	<div class="trader-table-row">
		<div class="adr"><a href="https://www.gmx.house/arbitrum/account/<%=trader.address %>" target="_blank"><%= trader.address %></a></div>
		<div class="pnl"><%= (stats[:pnl] / 10**30).to_i %></div>
		<div class="pnl_perc"><%= (stats[:pnl_percentage] / 100.0).round(2) %></div>
		<div class="winrate"><%= (stats[:winrate] * 100).round(2) %>%</div>
		<div class="trade_count"><%= stats[:trade_count] %></div>
		<div class="std"><%= trader.std.round(2) %></div>	
	</div>

<% end %>